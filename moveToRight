#include <iostream>
#include <stdio.h>
const int len = 5;
#pragma warning(disable : 4996)

int main()
{
    int temp;
    
    printf("Array length is %d ", len);

    int myArray[len];
    int result[len];

    printf("\nEnter array values \n");
    for (int i = 0; i < len; i++)
    {
        scanf("%d", &myArray[i]);
    }



    for (int i = 1; i < len; i++)
    {
        for (int j = 0; j < len; j++)
        {
            if (j == 0)
            {
                result[j] = myArray[len-1];
                continue;
            }
            result[j] = myArray[j - 1];
        }
    }

    for (int i = 0; i < len; i++)
    {
        printf("%d", result[i]);
    }

    return 0;
}
