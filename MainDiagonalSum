#include <iostream>
#include <stdio.h>

//Кузнецов Андрей, 15 группа
//Вычисление суммы элементов главной диагонали матрицы
int main()
{
	const int n = 3;

	int s = 0;
	
	int myArray[n][n] = { 1, 2, 3
	, 4, 10, 6
	, 7, 8, 9};

	for (int i = 0; i < n; i++)
		s += myArray[i][i];
	
	printf("%d", s);
}
