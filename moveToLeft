#include <iostream>
#include <stdio.h>
const int len = 5;
#pragma warning(disable : 4996)

int main()
{
    int range;

    printf("Array length is %d ", len);
    printf("\nChoose the range: ");
    scanf("%d", &range);
    if (range >= 5)
        printf("Error: range is bigger or equal to array lenght");

    int myArray[len];
    int result[len];

    printf("\nEnter array values \n");
    for (int i = 0; i < len; i++)
    {
        scanf("%d", &myArray[i]);
    }



    for (int j = 0; j <= len -1; j++)
    {
        if (j + range > len - 1)
        {
            result[j] = myArray[len -1 - j];
            continue;
        }
        result[j] = myArray[j + range];
    }

    for (int i = 0; i < len; i++)
    {
        printf("%d", result[i]);
    }

    return 0;
}
