#include <iostream>
#include <stdio.h>

using namespace std;

//Кузнецов Андрей
//Если n < 10, то M = n!, иначе M = n^k, где k подаётся с ввода

int main()
{
	int n = 5, max = INT_MIN;
	int* arr = new int[n] {1, 2, 3, 4, 6};

	for (int i = 0; i < n; i++)
		if (arr[i] > max)
			max = arr[i];
	if (max % 2 == 0)
		printf("Max number is even");
	else
		printf("Max number is not even");
}
