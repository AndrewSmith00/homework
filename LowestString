#include <iostream>
#include <stdio.h>

using namespace std;

int const n = 2;

int const m = 3;

int main()
{
	int min = 0, sum = 0, ndx;

	int myArray[3][3];

	for (int i = 0; i < n; i++)
	{
		for (int j = 0; j < m; j++)
		{
			scanf_s("%d", &myArray[i][j]);
		}
	}

	for (int i = 0; i < m; i++)
	{
		min += myArray[0][i];
		ndx = 0;
	}

	for (int i = 1; i < n; i++)
	{
		for (int j = 0; j < m; j++)
		{
			sum += myArray[i][j];
		}
		if (sum < min)
		{
			min = sum;
			ndx = n;
		}
	}

	printf("\nNumber of lowest string is %d\nSum of elements is %d", ndx, min);
}
