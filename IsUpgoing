#include <iostream>

int main()
{
    int n;
    int i = 1;
    int firstNum;
    int secondNum;
    bool flag = 1;

    printf("Enter amount of numbers: \n");
    scanf_s("%d", &n);
    int amount = n;

    printf("Enter a number: \n");
    scanf_s("%d", &firstNum);

    while (i < amount)
    {
        printf("Enter a number: \n");
        scanf_s("%d", &secondNum);
        if (secondNum > firstNum)
        {
            firstNum = secondNum;
            i++;
        }
        else
        {
            flag = 0;
        }
    }
    if (flag)
        printf("Subsequence is upqoing");
    else
        printf("Subsequence is not upqoing");
}
