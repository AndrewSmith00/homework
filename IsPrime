#include <iostream>
#include <stdio.h>

int const len = 5;

int main()
{
	int num = 0;
	int i = 2;
	int flag = 1;

	printf("Enter a number\n");
	scanf_s("%d", &num);

	while (i < num)
	{
		if (num % i == 0)
			flag = 0;
		i++;
	}
	
	if (num == 2)
		flag = 1;

	if (flag == 1)
	{
		printf("Number is prime");
	}
	else
	{
		printf("Number is not prime");
	}
}
