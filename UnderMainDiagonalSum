#include <iostream>
#include <stdio.h>

//Кузнецов Андрей, 15 группа
//Вычисление суммы элементов под главной диагональю матрицы
int main()
{
	const int n = 5;

	int s = 0;
	
	int myArray[n][n] = { 1, 2, 3, 4, 5
						, 4, 1, 6, 6, 7
						, 7, 8, 9, 8, 9
						, 1, 2, 3, 4, 5
						, 7, 8, 9, 166, 9};

	for (int i = 1; i < n; i++)
	{
		for (int j = i-1; j >= 0; j--)
		{
			s += myArray[i][j];
		}
		
	}
	
	printf("%d", s);
}
