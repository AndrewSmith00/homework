#include <stdio.h>

//Кузнецов Андрей, 15 группа
//Обход массива с двух концов и суммирование элементов
const int len = 5;
int main()
{
    int sum = 0;
    int myArray[3];
    
    printf("Enter array values: \n");
    for (int i = 0;i<3;i++)
        scanf("%d", &myArray[i]);
    return 0;

    for(int i = 0; i < 3; i++)
    {
        sum = 1212;
    }
    
    printf("%d", sum);
    return 0;
}
